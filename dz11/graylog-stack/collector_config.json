{
  "name": "default-log-config",
  "inputs": [
    {
      "type": "file",
      "path": "/var/log/nginx/access.log",
      "streams": ["nginx-stream"]
    },
    {
      "type": "file",
      "path": "/var/log/php7.4-fpm.log",
      "streams": ["php-fpm-stream"]
    },
    {
      "type": "file",
      "path": "/var/log/mysql/error.log",
      "streams": ["mysql-stream"]
    }
  ]
}